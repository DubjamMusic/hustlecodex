{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.hustlecodex.com"
        }
      ],
      "destination": "https://hustlecodex.com/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.hustlecodex.co.uk"
        }
      ],
      "destination": "https://hustlecodex.co.uk/:path*",
      "permanent": true
    }
  ]
}
